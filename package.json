{
  "name": "object-parser",
  "main": "object-parser.js",
  "version": "0.0.1-alpha.2",
  "description": "Multiple parse/stringify function",

  "scripts": {
    "cover": "istanbul cover -- minijasminenode2 test/spec.js --verbose",

    "Travis-CI commands":"",
      "test": "$(jqn package scripts.cover)",
      "posttest": "zuul test/spec.js --report",

    "Bower build":"",
      "build": "browserify $(jqn package name) -s OP | uglifyjs -cm > $(jqn package name).min.js",
      "postbuild": "wc -c $(jqn bower main)",

    "Bower update":"",
      "update": "git tag v$(jqn bower version) &&  git push --tags"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build && npm run cover"
    }
  },
  "files": [
    "index.js",
    "object-parser",
    "lib"
  ],

  "dependencies": {
    "json5": "^0.4.0",
    "json_ml": "0.0.1-alpha.4",
    "yamljs": "^0.2.2"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.2.0",
    "ghooks": "^0.3.2",
    "istanbul": "^0.3.14",
    "jasmine": "^2.3.1",
    "jqn": "0.0.3",
    "minijasminenode2": "^1.0.0",
    "uglify-js": "^2.4.22",
    "zuul": "git://github.com/59naga/zuul.git#5592c010065562e674a0a5d08598359008a518bd"
  },

  "keywords": [
    "json5",
    "yaml",
    "jsonml"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/object-parser.git"
  },
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/59naga/object-parser/issues"
  },
  "homepage": "https://github.com/59naga/object-parser#readme"
}
